<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <record model="ir.filters" id="ficep_filter">
            <field name="name">ficep</field>
            <field name="model_id">ir.module.module</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="is_default" eval="1"/>
            <field name="domain">[
                '&amp;',
                ('state', '!=', 'uninstallable'),
                '|',
                ('category_id', '=', 'Political Association'),
                ('name', 'in', ['base','settings_improvement'])
]           </field>
            <field name="context">{}</field>
        </record>

    </data>
</openerp>
