<?xml version='1.0' encoding='UTF-8' ?>
<odoo noupdate="1">

    <record forcecreate="True" id="without_membership" model="membership.state">
        <field name="name">Without Status</field>
        <field name="code">without_membership</field>
        <field name="free_state" eval="True" />
        <field name="sequence" eval="100" />
    </record>
    <record forcecreate="True" id="member_candidate" model="membership.state">
        <field name="name">Member Candidate</field>
        <field name="code">member_candidate</field>
        <field name="sequence" eval="90" />
    </record>
    <record forcecreate="True" id="former_member" model="membership.state">
        <field name="name">Former Member</field>
        <field name="code">former_member</field>
        <field name="sequence" eval="80" />
    </record>
    <record forcecreate="True" id="member_committee" model="membership.state">
        <field name="name">Members Committee</field>
        <field name="code">member_committee</field>
        <field name="sequence" eval="70" />
    </record>
    <record forcecreate="True" id="former_member_committee" model="membership.state">
        <field name="name">Former Member Committee</field>
        <field name="code">former_member_committee</field>
        <field name="sequence" eval="70" />
    </record>
    <record forcecreate="True" id="supporter" model="membership.state">
        <field name="name">Supporter</field>
        <field name="code">supporter</field>
        <field name="free_state" eval="True" />
        <field name="sequence" eval="60" />
    </record>
    <record forcecreate="True" id="member" model="membership.state">
        <field name="name">Member</field>
        <field name="code">member</field>
        <field name="sequence" eval="50" />
    </record>
    <record forcecreate="True" id="refused_member_candidate" model="membership.state">
        <field name="name">Refused Member Candidate</field>
        <field name="code">refused_member_candidate</field>
        <field name="sequence" eval="20" />
    </record>
    <record forcecreate="True" id="former_supporter" model="membership.state">
        <field name="name">Former Supporter</field>
        <field name="code">former_supporter</field>
        <field name="free_state" eval="True" />
        <field name="sequence" eval="20" />
    </record>
    <record forcecreate="True" id="expulsion_former_member" model="membership.state">
        <field name="name">Former Member (expulsion)</field>
        <field name="code">expulsion_former_member</field>
        <field name="sequence" eval="10" />
    </record>
    <record forcecreate="True" id="resignation_former_member" model="membership.state">
        <field name="name">Former Member (resignation)</field>
        <field name="code">resignation_former_member</field>
        <field name="sequence" eval="10" />
    </record>
    <record
        forcecreate="True"
        id="inappropriate_former_member"
        model="membership.state"
    >
        <field name="name">Former Member (inappropriate)</field>
        <field name="code">inappropriate_former_member</field>
        <field name="sequence" eval="10" />
    </record>
    <record forcecreate="True" id="break_former_member" model="membership.state">
        <field name="name">Former Member (break)</field>
        <field name="code">break_former_member</field>
        <field name="sequence" eval="10" />
    </record>

</odoo>
