<odoo>

    <record model="ir.ui.view" id="res_partner_form_view">
        <field name="name">res.partner.form (in mozaik_mandate_show_website)</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="mozaik_mandate.res_partner_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//page[@name='mandates']/span" position="before">
                <group>
                    <field name="no_show_mandates" />
                </group>
            </xpath>
        </field>
    </record>



</odoo>
