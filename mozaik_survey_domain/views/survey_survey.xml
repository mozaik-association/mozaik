<odoo>

    <record model="ir.ui.view" id="survey_survey_form_view">
        <field name="name">survey.survey.form (in mozaik_survey_domain)</field>
        <field name="model">survey.survey</field>
        <field name="inherit_id" ref="mozaik_security_ama.survey_survey_form_view" />
        <field name="arch" type="xml">
           <xpath expr="//group[@name='int_instances']" position="after">
                <group name="domain" string="Target domain">
                    <field
                        name="domain"
                        widget="domain"
                        options="{'model': 'res.partner'}"
                    />
                </group>
            </xpath>
        </field>
    </record>



</odoo>
