<odoo>

    <record model="ir.ui.view" id="res_partner_form_view">
        <field name="name">res.partner.form (in mozaik_involvement_thesaurus)</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="mozaik_involvement.res_partner_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='creation_time']" position="after">
                <field name="interest_ids" widget="many2many_tags" />
            </xpath>
        </field>
    </record>



</odoo>
