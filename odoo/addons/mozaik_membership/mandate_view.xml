        <record id="sta_candidature_form_view" model="ir.ui.view">
            <field name="name">sta.candidature.form (mozaik_membership)</field>
            <field name="model">sta.candidature</field>
            <field name="inherit_id" ref="mozaik_mandate.sta_candidature_form_view" />
            <field name="groups_id" eval="[(6,0,[
                ref('mozaik_base.res_groups_developper'),
            ])]"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='moreinfo']" position="inside">
                    <field name="partner_instance_id"/>
