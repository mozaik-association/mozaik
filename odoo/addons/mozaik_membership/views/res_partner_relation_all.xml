<odoo>

    <record model="ir.ui.view" id="res_partner_relation_all_tree_view">
        <field name="name">res.partner.relation.all.tree (in mozaik_membership)</field>
        <field name="model">res.partner.relation.all</field>
        <field name="inherit_id" ref="partner_multi_relation.tree_res_partner_relation_all"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='this_partner_id']" position="after">
                <field name="this_instance_id" groups="base.group_erp_manager"/>
            </xpath>

            <xpath expr="//field[@name='other_partner_id']" position="after">
                <field name="other_instance_id" groups="base.group_erp_manager"/>
            </xpath>
        </field>
    </record>

</odoo>
