<odoo>

    <record id="allow_duplicate_form_view" model="ir.ui.view">
        <field name="name">Allow.duplicate.wizard (in mozaik_duplicate)</field>
        <field name="model">allow.duplicate.wizard</field>
        <field name="arch" type="xml">
            <form>
                <strong>
                    Selected duplicates are allowed. Are you sure?
                </strong>
                <footer>
                    <button string="Ok" name="button_allow_duplicate" type="object" class="oe_highlight" />
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>