<odoo>

    <record model="ir.ui.view" id="generic_mandate_form_view">
        <field name="name">generic.mandate.form (in mozaik_mandate_category_sequence)</field>
        <field name="model">generic.mandate</field>
        <field name="inherit_id" ref="mozaik_mandate.generic_mandate_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='deadline_date']" position="after">
                <field name="sequence"/>
            </xpath>
        </field>
    </record>

</odoo>
