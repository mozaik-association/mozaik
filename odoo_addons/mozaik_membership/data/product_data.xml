<?xml version='1.0' encoding='UTF-8' ?>
<openerp>
    <data noupdate="0">

        <record id="membership_product_category" model="product.category">
            <field name="name">Subscription</field>
            <field name="parent_id" ref="product.product_category_all" />
        </record>

    </data>

    <data noupdate="1">

        <record id="membership_product_free" model="product.product">
            <field name="name">Free Subscription</field>
        </record>
        <record id="membership_product_first" model="product.product">
            <field name="name">First Subscription</field>
            <field name="list_price" eval="10.0" />
        </record>
        <record id="membership_product_isolated" model="product.product">
            <field name="name">Usual Subscription</field>
            <field name="list_price" eval="25.0" />
        </record>
        <record id="membership_product_live_together" model="product.product">
            <field name="name">Cohabitant Subscription</field>
            <field name="list_price" eval="18.0" />
        </record>
        <record id="membership_product_other" model="product.product">
            <field name="name">Reduced Subscription</field>
            <field name="list_price" eval="12.0" />
        </record>
        <record id="membership_product_undefined" model="product.product">
            <field name="name">Undefined Subscription</field>
        </record>

    </data>

    <data noupdate="0">

        <record id="membership_product_free" model="product.product">
            <field name="name">Free Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
            <field name="list_price" eval="0.0" />
        </record>
        <record id="membership_product_first" model="product.product">
            <field name="name">First Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
        </record>
        <record id="membership_product_isolated" model="product.product">
            <field name="name">Usual Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
        </record>
        <record id="membership_product_live_together" model="product.product">
            <field name="name">Cohabitant Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
        </record>
        <record id="membership_product_other" model="product.product">
            <field name="name">Reduced Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
        </record>
        <record id="membership_product_undefined" model="product.product">
            <field name="name">Undefined Subscription</field>
            <field name="categ_id" ref="membership_product_category" />
            <field name="membership" eval="1" />
            <field name="type">service</field>
            <field name="list_price" eval="0.0" />
        </record>

    </data>
</openerp>
