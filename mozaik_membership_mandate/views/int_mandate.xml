<odoo>

    <record model="ir.ui.view" id="int_mandate_form_view">
        <field name="name">int.mandate.form (in mozaik_membership_mandate)</field>
        <field name="model">int.mandate</field>
        <field name="inherit_id" ref="mozaik_mandate.int_mandate_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='int_assembly_id']" position="after">
                <field name="mandate_instance_id"/>
            </xpath>
            <xpath expr="//group[@name='moreinfo']" position="inside">
                <field name="partner_instance_ids" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>

</odoo>
