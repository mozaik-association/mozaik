<odoo>

    <record id="intm_paul_regional" model="int.mandate">
        <field name="mandate_category_id" ref="mc_membre_assemblee_regionale"/>
        <field name="designation_int_assembly_id" ref="mozaik_structure.int_assembly_01"/>
        <field name="int_assembly_id" ref="mozaik_structure.int_assembly_02"/>
        <field name="start_date" eval="(DateTime.today() - relativedelta(years=1)).strftime('%Y-%m-%d')"/>
        <field name="deadline_date" eval="(DateTime.today() + relativedelta(years=6)).strftime('%Y-12-31')"/>
        <field name="partner_id" ref="mozaik_coordinate.res_partner_paul"/>
    </record>

    <record id="intm_thierry_secretaire_done" model="int.mandate">
        <field name="mandate_category_id" ref="mc_secretaire_regional"/>
        <field name="designation_int_assembly_id" ref="mozaik_structure.int_assembly_01"/>
        <field name="int_assembly_id" ref="mozaik_structure.int_assembly_02"/>
        <field name="start_date" eval="(DateTime.today() - relativedelta(years=8)).strftime('%Y-%m-%d')"/>
        <field name="deadline_date" eval="(DateTime.today() - relativedelta(years=1)).strftime('%Y-12-31')"/>
        <field name="end_date" eval="(DateTime.today() - relativedelta(years=1)).strftime('%Y-12-31')"/>
        <field name="partner_id" ref="mozaik_coordinate.res_partner_thierry"/>
    </record>

</odoo>
