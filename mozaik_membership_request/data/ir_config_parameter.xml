<?xml version='1.0' encoding='UTF-8' ?>
<odoo noupdate="1">

    <record id="mr_required_age" model="ir.config_parameter">
        <field name="key">mr_required_age</field>
        <field name="value">16</field>
    </record>
     <record id="number_days_accept_member_committee" model="ir.config_parameter">
        <field name="key">number_days_accept_member_committee</field>
        <field name="value">30</field>
    </record>
    <record id="number_days_member_candidate_to_supporter" model="ir.config_parameter">
        <field name="key">number_days_member_candidate_to_supporter</field>
        <field name="value">90</field>
    </record>

    <record id="involvement_categories_from_mr" model="ir.config_parameter">
        <field name="key">involvement_categories_from_mr</field>
        <field name="value">[petition, voluntary, newsletter, notification]</field>
    </record>

    <record id="allow_no_ic_from_mr" model="ir.config_parameter">
        <field name="key">allow_no_ic_from_mr</field>
        <field name="value">True</field>
    </record>
    </odoo>
