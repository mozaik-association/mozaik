<odoo>

    <record model="ir.ui.view" id="membership_request_form_view">
        <field
            name="name"
        >membership.request.form (in mozaik_membership_banking_mandate)</field>
        <field name="model">membership.request</field>
        <field
            name="inherit_id"
            ref="mozaik_membership_request.membership_request_form_view"
        />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="bank" string="Bank">
                    <group>
                        <field name="bank_account_number" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>



</odoo>
