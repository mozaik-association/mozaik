<odoo>

    <record model="ir.ui.view" id="event_type_form_view">
        <field name="name">event.type.form (in mozaik_event_question_thesaurus)</field>
        <field name="model">event.type</field>
        <field
            name="inherit_id"
            ref="website_event_questions.event_type_view_form_inherit_question"
        />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='question_type']" position="before">
                    <field name="interest_ids" widget="many2many_tags" />
            </xpath>
        </field>
    </record>


</odoo>
